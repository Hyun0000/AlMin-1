		<typeAlias type="com.kh.almin.recruit.model.vo.Recruit" alias="Recruit" />
		<typeAlias type="com.kh.almin.comments.model.vo.CommentsCompany" alias="CommentsCompany" />
		<typeAlias type="com.kh.almin.district.model.vo.District" alias="District" />
		<typeAlias type="com.kh.almin.applicant.model.vo.Applicant" alias="Applicant" />
